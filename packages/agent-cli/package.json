{
  "name": "@kb-labs/agent-cli",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "CLI commands for KB Labs Agents V2. Provides agent:run-v2 command.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugin-manifest": {
      "import": "./dist/manifest.js",
      "types": "./dist/manifest.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "kb": {
    "manifest": "./dist/manifest.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "pretype-check": "pnpm --filter @kb-labs/agent-cli build",
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kb-labs/agent-contracts": "workspace:*",
    "@kb-labs/agent-core": "workspace:*",
    "@kb-labs/agent-history": "workspace:*",
    "@kb-labs/agent-tools": "workspace:*",
    "@kb-labs/agent-tracing": "workspace:*",
    "@kb-labs/sdk": "link:../../../kb-labs-sdk/packages/sdk",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@kb-labs/plugin-runtime": "link:../../../kb-labs-plugin/packages/plugin-runtime",
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
