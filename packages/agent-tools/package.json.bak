{
  "name": "@kb-labs/agent-tools",
  "version": "0.1.0",
  "description": "Agent management tools for planning, coordination, and execution",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kb-labs/core-platform": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "kb-labs",
    "agents",
    "tools",
    "planning",
    "coordination"
  ],
  "author": "KB Labs",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
