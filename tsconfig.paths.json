{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@kb-labs/agent-contracts": [
        "packages/agent-contracts/src/index.ts"
      ],
      "@kb-labs/agent-contracts/*": [
        "packages/agent-contracts/src/*"
      ],
      "@kb-labs/agent-core": [
        "packages/agent-core/src/index.ts"
      ],
      "@kb-labs/agent-core/*": [
        "packages/agent-core/src/*"
      ],
      "@kb-labs/agent-history": [
        "packages/agent-history/src/index.ts"
      ],
      "@kb-labs/agent-history/*": [
        "packages/agent-history/src/*"
      ],
      "@kb-labs/agent-tools": [
        "packages/agent-tools/src/index.ts"
      ],
      "@kb-labs/agent-tools/*": [
        "packages/agent-tools/src/*"
      ],
      "@kb-labs/agent-tracing": [
        "packages/agent-tracing/src/index.ts"
      ],
      "@kb-labs/agent-tracing/*": [
        "packages/agent-tracing/src/*"
      ],
      "@kb-labs/core-platform": [
        "../kb-labs-core/packages/core-platform/src/index.ts"
      ],
      "@kb-labs/core-platform/*": [
        "../kb-labs-core/packages/core-platform/src/*"
